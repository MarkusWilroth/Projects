<Window x:Class="ANN_Test.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ANN_Test"
        mc:Ignorable="d"
        Title="VCTTestZone" Height="450" Width="800">
    <Grid>
        <Border Height="434"  Width="800" Margin="0,0,0,0" BorderThickness="2" BorderBrush="#FF6ABE30" Background="#FF22825B" />
        <Label Content="VCT TEST ZONE" HorizontalAlignment="Center" Margin="0,20,0,0" VerticalAlignment="Top" FontWeight="Bold" FontSize="34" Foreground="White" FontFamily="Times New Roman"/>
        <Label x:Name="TxtProg" Content="" HorizontalAlignment="Left" Margin="520,372,0,0" VerticalAlignment="Top" FontWeight="Bold" FontSize="20" Foreground="White" FontFamily="Times New Roman" />

        <Border Height="280"  Width="600" Margin="0,20,0,0" BorderThickness="2" BorderBrush="#FF6ABE30" Background="#FF37946E"/>
        <Button x:Name="BtnCreateANN" Content="Create Network" Margin="0,365,0,0" Width="200" Height="40" IsEnabled="True" FontSize="25" Foreground="White" FontFamily="Times New Roman" FontWeight="Bold" BorderThickness="2" BorderBrush="#FF6ABE30" Background="#FF37946E" Click="BtnCreateNetwork_Click"/>
        <Button x:Name="BtnCreateCom" Content="Create Command" Margin="-440,-300,0,0" Width="160" Height="30" FontSize="18" Foreground="White" FontFamily="Times New Roman" FontWeight="Bold" BorderThickness="2"  BorderBrush="#FF6ABE30" Background="#FF51A482" Click="BtnCreateCommand_Click" VerticalAlignment="Center" HorizontalAlignment="Center"/>
        <Button x:Name="BtnTestANN" Content="Test Network" Margin="440,-300,0,0" Width="160" Height="30" FontSize="18" Foreground="White" FontFamily="Times New Roman" FontWeight="Bold" BorderThickness="2"  BorderBrush="#FF6ABE30" Background="#FF51A482" Click="BtnTestNetwork_Click" VerticalAlignment="Center" HorizontalAlignment="Center"/>
        <Button x:Name="BtnSave" Content="Save" Margin="-700,-100,0,0" Width="70" Height="40" FontSize="29" Foreground="White" FontFamily="Times New Roman" FontWeight="Bold" BorderThickness="2"  BorderBrush="#FF6ABE30" Background="#FF51A482" Click="BtnSave_Click" VerticalAlignment="Center" HorizontalAlignment="Center"/>
        <Button x:Name="BtnSaveAs" Content="Save As" Margin="-700,0,0,0" Width="80" Height="40" FontSize="20" Foreground="White" FontFamily="Times New Roman" FontWeight="Bold" BorderThickness="2"  BorderBrush="#FF6ABE30" Background="#FF51A482" Click="BtnSaveAs_Click" VerticalAlignment="Center" HorizontalAlignment="Center"/>
        <Button x:Name="BtnLoad" Content="Load" Margin="-700,100,0,0" Width="80" Height="40" FontSize="29" Foreground="White" FontFamily="Times New Roman" FontWeight="Bold" BorderThickness="2"  BorderBrush="#FF6ABE30" Background="#FF51A482" Click="BtnLoad_Click" VerticalAlignment="Center" HorizontalAlignment="Center"/>
        <Button x:Name="BtnLoadANN" Content="Load Network" Margin="-350,365,0,0" Width="120" Height="40" FontSize="17" Foreground="White" FontFamily="Times New Roman" FontWeight="Bold" BorderThickness="2"  BorderBrush="#FF6ABE30" Background="#FF51A482" Click="BtnLoadANN_Click" VerticalAlignment="Center" HorizontalAlignment="Center"/>

        <ScrollBar x:Name="Scroller" Height="280" Width="10" Margin="640,20,0,0" Visibility="Visible" BorderBrush="#FF6ABE30" Background="#FF37946E"  LargeChange="0.1" BorderThickness="2,2,2,2" ValueChanged="Scroller_ValueChanged"/>

